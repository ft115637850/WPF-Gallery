<Window x:Class="ClientWindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:ClientWindow"
        xmlns:my="clr-namespace:WpfGallery;assembly=WpfGallery"
        Title="MainWindow" Height="600" Width="800">
    <Grid Background="Azure">
        <my:Gallery ImgsSrc="{Binding MyImgsSrc}" RotationDuration="00:00:0.3" IsCircular="False"></my:Gallery>
    </Grid>
</Window>
